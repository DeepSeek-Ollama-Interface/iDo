{
  "name": "deepseek-ollama-interface",
  "private": true,
  "version": "0.0.0",
  "main": "./electron/main.cjs",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "dev": "NODE_ENV=development concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "dev:win": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "vite build",
    "preview": "vite preview",
    "dist:linux": "npm run build && electron-builder --linux AppImage",
    "dist:win": "electron-packager . iDO --platform=win32 --arch=x64 --out=dist/ --overwrite",
    "dist:all": "npm run build && electron-builder --win nsis --linux AppImage"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.4",
    "github-url-to-object": "^4.0.6",
    "ms": "^2.1.3",
    "path": "^0.12.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5",
    "tailwindcss": "^4.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "daisyui": "^5.0.0-beta.6",
    "electron": "^34.1.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "vite": "^6.1.0",
    "wait-on": "^8.0.2"
  }
}
