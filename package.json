{
  "name": "deepseek-ollama-interface",
  "private": true,
  "version": "1.0.3",
  "author": "https://ido.vin",
  "main": "./electron/main.cjs",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/DeepSeek-Ollama-Interface/iDo"
  },
  "scripts": {
    "lint": "eslint .",
    "dev": "npm run build && cross-env NODE_ENV=development concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "vite build",
    "preview": "vite preview",
    "dist:linux": "npm run build && electron-packager . iDo --platform=linux --arch=x64 --out=dist/ --overwrite --icon=./build/icons/icon.png",
    "dist:win": "electron-packager . iDO --platform=win32 --arch=x64 --out=dist/ --overwrite --icon=./build/icons/icon.ico",
    "dist:all": "npm run build && electron-builder --win nsis --linux AppImage",
    "npm": "npm i && cd ./backend && npm i && cd .."
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.4",
    "docx": "^9.1.1",
    "github-url-to-object": "^4.0.6",
    "jsdom": "^26.0.0",
    "ms": "^2.1.3",
    "nodejs-whisper": "^0.2.6",
    "os-utils": "^0.0.14",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5",
    "tailwindcss": "^4.0.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^1.3.2",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "daisyui": "^5.0.0-beta.6",
    "electron": "^34.1.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "vite": "^6.1.0",
    "wait-on": "^8.0.2"
  }
}
